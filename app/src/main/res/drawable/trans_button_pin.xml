<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <gradient android:startColor="#5999B998"
                android:centerColor="#5999B998"
                android:endColor="#99B998"
                android:angle="90" />
            <stroke
                android:width="1dp"
                android:color="@color/color_number" />
            <corners
                android:radius="0dp" />

        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1dp"
                android:color="@color/color_number" />

            <corners android:radius="0dp" />
            <gradient android:startColor="#5999B998"
                android:centerColor="#5999B998"
                android:endColor="#5999B998"
                android:angle="90" />


        </shape>
    </item>
</selector>
